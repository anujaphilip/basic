4..........<html>
<script language="JavaScript"> 
function f()
{
 var a,b,c,n;
 n=parseInt(prompt("Enter Number"));
 a=0;
 b=1;
 document.write(a);
 document.write(b);
 for(i=3; i<=n; i++)
 {
   c=(a+b);
   document.write(c);
   a=b;
   b=c;
 }
}
</script>
<body>
<a href="" onClick="f()">fibonacci series</a>
</body>
</html>


