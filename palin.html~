JavaScript Code :
view plaincopy to clipboardprint?

    function test_prime(n)  
    {  
      
      if (n===1)  
      {  
        return false;  
      }  
      else if(n === 2)  
      {  
        return true;  
      }else  
      {  
        for(var x = 2; x < n; x++)  
        {  
          if(n % x === 0)  
          {  
            return false;  
          }  
        }  
        return true;    
      }  
    }  


<html>  
<head><TITLE></TITLE>  
<script language="JavaScript">  
var a=prompt("enter a value");  
alert(a);  
var c=0;  
  
 for(var i=2;i<a;i++)  
  {  
   if(a%i==0)  
   {  
    c=i;  
    //alert("Now C IS:"+c);  
   }  
  }  
  if(c==0)  
  {  
   alert("given no is prime");  
  }  
  else  
  {  
   alert("given no is not a prime");  
  }  
</script>  
</head>  
<body></body>  
</html>  
